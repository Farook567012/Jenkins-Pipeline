pipeline {
  agent any
  stages {
    stage('Plan') {
      steps {
        echo 'Plan for pipeline'
      }
    }

    stage('Code') {
      steps {
        echo 'Prepare for code'
      }
    }

    stage('Build') {
      steps {
        echo 'Build for code'
      }
    }

    stage('Test') {
      steps {
        echo 'Test for code'
      }
    }

    stage('Release') {
      steps {
        echo 'Release code'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploy the code'
      }
    }

  }
}